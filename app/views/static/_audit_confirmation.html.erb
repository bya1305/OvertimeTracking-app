<% pending_audits.each do |a| %>
  <%= link_to "I confirm that I did not perform any overtime for the week of: #{a.start_date} through #{a.start_date + 6.days}", confirm_audit_log_path(a),  class: 'btn btn-primary btn-block btn-lg', id: "confirm_#{a.id}", data: { confirm: "Are you sure you want to confirm that you did not perform any overtime?" } %>
<% end %>
